<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Reservation">
	<insert id="memberResInsert">
	
	</insert>
	<insert id="nmResInsert" parameterType="resdto">
		insert into reservation (idx,rem,resdate,store,restime,restable,totalprice,fsingle,
		nm_hp,nm_pass) values(seq_reservation.nextval,0,to_date(#{resdate},'DD/MM/RR'),
		#{store},#{restime},#{restable},#{totalprice},#{fsingle},#{nm_ph},#{nm_pass})
	</insert>	
</mapper>